api_platform:
    title: Hello API Platform
    version: 1.0.0
    defaults:
        stateless: true
        cache_headers:
            vary: ['Content-Type', 'Authorization', 'Origin']

    formats:
        jsonld:
            mime_types: ['application/ld+json']
        jsonapi:
            mime_types: ['application/vnd.api+json']
